{
  "name": "x-zolezzi/my-mic-plugin-cake",
  "description": "Plugin CakePHP for MY MAKE IT CREATIVE",
  "type": "cakephp-plugin",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "version": "1.0.0",
  "require": {
    "cakephp/cakephp": "^3.5 || ^4.0",
    "league/glide": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "MakeItCreative\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MakeItCreative\\Test\\": "tests/",
      "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
    }
  }
}
